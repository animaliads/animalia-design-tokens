{
  "source": ["src/brands/fluig/**/*.json"],
  "platforms": {
    "css": {
      "transformGroup": "css",
      "buildPath": "dist/brands/fluig/css/",
      "files": [{
        "destination": "brand-tokens.css",
        "format": "css/variables"
      }]
    },
    "scss": {
      "transformGroup": "scss",
      "buildPath": "dist/brands/fluig/scss/",
      "files": [{
        "destination": "brand-tokens.scss",
        "format": "scss/variables"
      }]
    }
  }
}
